{
  "name": "late-stage-api",
  "module": "commonjs",
  "scripts": {
    "migrate:dev": "npx prisma migrate dev --name",
    "migrate:deploy": "npx prisma migrate deploy",
    "generate": "npx prisma generate",
    "build": "npm run generate && npm run migrate:deploy",
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon -L --exec npm run start -e ts,json,js",
    "prod": "NODE_ENV=production npm run start",
    "start": "./node_modules/.bin/tsm index.ts --project ./tsconfig.json",
    "test": "./node_modules/.bin/forever start -a -c \"npm run start\" --uid \"employees-market-api\" ./ && ./node_modules/.bin/nodemon -L --exec ./node_modules/.bin/tsm ./node_modules/uvu/bin.js tests -e ts,json,js",
    "test:stop": "./node_modules/.bin/forever stop \"employees-market-api\""
  },
  "dependencies": {
    "@prisma/client": "^3.6.0",
    "altair-fastify-plugin": "^4.2.1",
    "dotenv": "^10.0.0",
    "fastify": "^3.25.0",
    "fastify-cors": "^6.0.2",
    "fastify-file-upload": "^3.0.1",
    "fastify-multer": "^2.0.2",
    "fastify-multipart": "^5.2.1",
    "fastify-static": "^4.5.0",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.14.1",
    "mercurius": "^8.12.0",
    "multer": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.18.2"
  },
  "devDependencies": {
    "@types/graphql-fields": "^1.3.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.13",
    "@types/sinon": "^10.0.6",
    "forever": "^4.0.1",
    "graphql-fields": "^2.0.3",
    "nodemon": "^2.0.15",
    "prisma": "^3.6.0",
    "sinon": "^12.0.1",
    "ts-node": "^10.4.0",
    "tsm": "^2.2.1",
    "typescript": "^4.5.4",
    "uvu": "^0.5.2"
  }
}
