datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

generator typegraphql {
  provider = "typegraphql-prisma"
}

model User {
  id              String  @id @default(cuid())
  email           String  @unique
  username        String  @unique
  password        String
  emailIsVerified Boolean
  cred            Int

  createdAt DateTime @default(now())
}
