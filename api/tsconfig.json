{
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "target": "es2020",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "alwaysStrict": true,
    "module": "es2020",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "include": [
    ".",
  ],
  "exclude": [
    "node_modules"
  ]
}